##ANL##
host mgt1 mgt1.ether mgt1.ether.alcf.anl.gov
    hostname mgt1.ether.alcf.anl.gov 
    LocalForward localhost:32074 sn1-mgmt.intrepid.alcf.anl.gov:32072
    LocalForward localhost:32073 sn1-mgmt.surveyor.alcf.anl.gov:32072
    LocalForward localhost:44343 mgt3-mgmt.ether.alcf.anl.gov:443
    LocalForward localhost:5000  mgt3-mgmt.ether.alcf.anl.gov:5000
    LocalForward localhost:5001  mgt3-mgmt.ether.alcf.anl.gov:5001
    LocalForward localhost:5002  mgt3-mgmt.ether.alcf.anl.gov:5002
    LocalForward localhost:2222  mgt3-mgmt.ether.alcf.anl.gov:22
    LocalForward localhost:5801  mgt3-mgmt.ether.alcf.anl.gov:5801
    LocalForward localhost:5901  mgt3-mgmt.ether.alcf.anl.gov:5901
    forwardagent yes
    controlmaster auto

host mgt2 mgt2.ether mgt2.ether.alcf.anl.gov
    hostname mgt2.ether.alcf.anl.gov 
    LocalForward localhost:32074 sn1-mgmt.intrepid.alcf.anl.gov:32072
    LocalForward localhost:32073 sn1-mgmt.surveyor.alcf.anl.gov:32072
    LocalForward localhost:44343 mgt3-mgmt.ether.alcf.anl.gov:443
    LocalForward localhost:5000  mgt3-mgmt.ether.alcf.anl.gov:5000
    LocalForward localhost:5001  mgt3-mgmt.ether.alcf.anl.gov:5001
    LocalForward localhost:5002  mgt3-mgmt.ether.alcf.anl.gov:5002
    LocalForward localhost:2222  mgt3-mgmt.ether.alcf.anl.gov:22
    forwardagent yes
    controlmaster auto

