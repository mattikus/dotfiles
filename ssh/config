###Classes###
host csc
  hostname classes.csc.lsu.edu
  user cs430414
  ControlMaster auto
#############

###vps###
host vps
  hostname mattikus.com
  user matt
  forwardAgent yes
  port 64000
  ControlMaster auto

host proxy-vps
  hostname mattikus.com
  user matt
  port 64000
  DynamicForward 8080
################

###Home Machines###
host *.mork *.local
  user matt
  forwardAgent yes
  forwardX11trusted yes
  ControlMaster auto
  port 64000

host home
  hostname sevinn.ath.cx
  user matt
  forwardAgent yes
  controlMaster auto
  port 64000

host mark mark.mork
  user matthew
  ControlPath ~/.ssh/master-%r@%h:%p
  PermitLocalCommand yes
##################


###bitbucket###
host bb
  hostname bitbucket.org
  compression yes
