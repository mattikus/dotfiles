host *
  ControlPath ~/.ssh/master-%r@%h:%p
  ControlPersist 4h

###vps###
host vps
  hostname mattikus.com
  user matt
  forwardAgent yes
  port 443
  ControlMaster auto

host vps-proxy
  hostname mattikus.com
  user matt
  port 443
  DynamicForward 8080
################

###Home Machines###
host *.mork *.local
  user matt
  forwardAgent yes
  forwardX11trusted yes
  ControlMaster auto
  port 64000

host mark mark.mork
  user matthew
  PermitLocalCommand yes
##################


###bitbucket###
host bb
  hostname bitbucket.org
  compression yes
###############

##ANL##
host *.anl.gov
  user mkemp

host login*.mcs login*.mcs.anl.gov
  user mkemp
  forwardagent yes
  ControlMaster auto

host mgt1 mgt1.ether mgt1.ether.alcf.anl.gov
  hostname mgt1.ether.alcf.anl.gov
  user mkemp
  forwardagent yes
  ControlMaster auto

host mgt2 mgt2.ether mgt2.ether.alcf.anl.gov
  hostname mgt2.ether.alcf.anl.gov
  user mkemp
  forwardagent yes
  ControlMaster auto

host namby namby.mcs namby.mcs.anl.gov
  hostname namby.mcs.anl.gov
  user mkemp
  forwardagent yes
  #LocalForward localhost:4000 status.alcf.anl.gov:4000
  ControlMaster auto

host status
  ProxyCommand ssh -A namby netcat status.alcf.anl.gov %p
  user mkemp
  ControlMaster auto

#host status status.alcf status.alcf.anl.gov
  #hostname status.alcf.anl.gov
