    ProxyCommand ssh -A mgt1.ether netcat mgt3.ether %p

host namby
    hostname namby.mcs.anl.gov
    forwardagent yes
    LocalForward localhost:2222 status.alcf.anl.gov:22
    LocalForward localhost:5050 status.alcf.anl.gov:5050
    controlmaster auto

host pamby
    hostname pamby.mcs.anl.gov
    forwardagent yes
    LocalForward localhost:2222 status.alcf.anl.gov:22
    LocalForward localhost:5050 status.alcf.anl.gov:5050
    controlmaster auto

host status
    ProxyCommand ssh -A namby netcat status.alcf.anl.gov %p

host *
user mkemp
ControlPath ~/.ssh/master-%r@%h:%p
